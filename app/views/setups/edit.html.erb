<h1>Editing Setup</h1>


<%= form_for(@setup) do |f| %>
  <div class="field">
    <%= f.label :name, "Edit Name", :class => "control-label" %>
    <%= f.text_field :name, autofocus: true, :class => "form-control" %>
  </div>

  <div class="actions">
    <%= f.submit "Next", :class => "btn setup-form-btn" %>
  </div>
<% end %>

<%= link_to 'Show', @setup %> |
<%= link_to 'Back', setups_path %>
