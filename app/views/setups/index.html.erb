<div style="vcenter">
  <h3>My Setups</h3>
</div>

<div class="grid row">
  <% @setups.each do |setup| %>
    <% if !setup.setup_images.blank? %>
      <div class="grid-item">
        <%= link_to image_tag(setup.setup_images.first.image.thumbBig.url, :class => "img-responsive"), setup %>
      </div>
    <% else %>
      <div class="grid-item">
        <%= link_to image_tag("add-image", :class => "img-responsive ph-image"), setup %>
      </div>
    <% end %>
  <% end %>
</div>
