%p#notice= notice
%h1 Parts
%table
  %thead
    %tr
      %th Setup
      %th Name
      %th Link
      %th{:colspan => "3"}
  %tbody
    - @parts.each do |part|
      %tr
        %td= part.setup_id
        %td= link_to Category.find(part.category_id).name, category_path(part.category_id)
        %td= part.name
        %td= part.link
        %td= link_to 'Show', part
        %td= link_to 'Edit', edit_part_path(part)
        %td= link_to 'Destroy', part, method: :delete, data: { confirm: 'Are you sure?' }
%br
= link_to 'New Part', new_part_path
